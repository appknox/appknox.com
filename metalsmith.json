{
    "metadata": {
        "title": "Appknox",
        "description": "Yiopep"
    },
    "plugins": {
        "metalsmith-watch": {
            "paths": {
                "${source}/**/*": true
            },
            "livereload": true
        },
        "metalsmith-jade": {
            "pretty": true
        },
        "metalsmith-permalinks": {
            "pattern": ":title"
        },
        "metalsmith-html-minifier": {},
        "metalsmith-serve": {
            "port": 8081,
            "verbose": true
        }
    }
}
